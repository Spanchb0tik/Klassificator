from PyQt6.QtWidgets import QApplication
from PyQt6 import uic
import requests
from bs4 import BeautifulSoup
import csv
def solve():
    url=form.inp.text()
    # Парсер всего текста
    form.outp.setText(url)# скобки запихнуть ответ
    
Form, Windows = uic.loadUiType("C:\\Users\davyd\OneDrive\Документы\Project X\project.ui")
win = QApplication([])
windows = Windows()
windows.setWindowTitle("Классификация сайтов")
form = Form()
form.setupUi(windows)
form.button.clicked.connect(solve)
windows.show()
win.exec()
